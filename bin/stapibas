#!/usr/bin/env php
<?php
namespace stapibas;
require_once __DIR__ . '/../data/config.php';
require_once 'stapibas/autoloader.php';

$db = new PDO($dbdsn, $dbuser, $dbpass);
$log = new Logger();

$uf = new Feed_UpdateFeeds();
$uf->db = $db;
$uf->log = $log;
$uf->updateAll();

$uf = new Feed_UpdateEntries();
$uf->db = $db;
$uf->log = $log;
$uf->updateAll();

?>
